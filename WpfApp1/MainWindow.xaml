<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="FileNameTextBox" HorizontalAlignment="Left" Margin="65,64,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="BrowseButton_Click" Content="Button" HorizontalAlignment="Left" Margin="245,62,0,0" VerticalAlignment="Top" Click="Button_Click"/>
        <Grid x:Name="gridRes" HorizontalAlignment="Left" Height="288" Margin="400,0,0,0" VerticalAlignment="Center" Width="335"/>
        <ListBox x:Name="fileListBox" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="65,99,500,35" MouseDoubleClick="fileListBox_getXMLFromList" />
    </Grid>
</Window>
